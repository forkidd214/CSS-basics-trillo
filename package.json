{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Play around with css flexbox",
  "main": "index.js",
  "scripts": {
    "compile:sass": "sass sass/main.scss css/style.css",
    "prefix:css": "postcss css/style.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "minify css/style.prefix.css > css/style.min.css",
    "build": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forkidd214/CSS-basics-trillo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/forkidd214/CSS-basics-trillo/issues"
  },
  "homepage": "https://github.com/forkidd214/CSS-basics-trillo#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "minify": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.9"
  }
}
